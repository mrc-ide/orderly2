{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Outpack git information",
    "description": "Information about source versioning with git",
    "version": "0.1.1",

    "type": "object",
    "properties": {
        "sha": {
            "type": ["string", "null"],
            "pattern": "^[0-9a-f]+$"
        },
        "branch": {
            "type": ["string", "null"]
        },
        "url": {
            "type": "array",
            "items": {
                "type": "string"
            }
        }
    }
}
